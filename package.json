{
    "name": "vscode-toolbox",
    "displayName": "Toolbox",
    "description": "A set of tools for developing.",
    "publisher": "caiqichang",
    "version": "1.0.0",
    "engines": {
        "vscode": "^1.87.0"
    },
    "categories": [
        "Other"
    ],
    "keywords": [
        "tool"
    ],
    "icon": "resources/logo.png",
    "extensionKind": [
        "ui",
        "workspace"
    ],
    "capabilities": {
        "untrustedWorkspaces": {
            "supported": true
        }
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/caiqichang/vscode-toolbox"
    },
    "main": "out/extension.js",
    "activationEvents": [
        "onStartupFinished"
    ],
    "contributes": {
        "viewsContainers": {
            "activitybar": [
                {
                    "id": "toolbox_bar",
                    "title": "Toolbox",
                    "icon": "resources/logo.png"
                }
            ]
        },
        "views": {
            "toolbox_bar": [
                {
                    "id": "toolbox_bar_view",
                    "name": "Tools",
                    "type": "webview"
                }
            ]
        }
    },
    "scripts": {
        "prepackage": "node script/pre-package.js && npm run compile",
        "package": "npx --package vsce vsce package -o build/",
        "compile": "tsc --project ./",
        "watch": "tsc --watch --project ./"
    },
    "devDependencies": {
        "@types/node": "^20.11.24",
        "@types/vscode": "^1.87.0",
        "typescript": "^5.3.3"
    },
    "dependencies": {
        "@vscode/codicons": "^0.0.35",
        "@vscode/webview-ui-toolkit": "^1.4.0",
        "vue": "^3.4.21"
    }
}